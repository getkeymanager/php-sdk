{
  "name": "getkeymanager/php-sdk",
  "version": "3.1.0",
  "description": "Official PHP SDK for License Management Platform API",
  "type": "library",
  "keywords": [
    "license",
    "license-management",
    "licensing",
    "software-licensing",
    "activation",
    "validation",
    "drm"
  ],
  "homepage": "https://getkeymanager.com",
  "license": "MIT",
  "authors": [
    {
      "name": "License Manager Team",
      "email": "dev@getkeymanager.com"
    }
  ],
  "support": {
    "email": "support@getkeymanager.com",
    "docs": "https://docs.getkeymanager.com",
    "issues": "https://github.com/getkeymanager/php-sdk/issues",
    "source": "https://github.com/getkeymanager/php-sdk"
  },
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^1.8",
    "squizlabs/php_codesniffer": "^3.7",
    "php-parallel-lint/php-parallel-lint": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "GetKeyManager\\SDK\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GetKeyManager\\SDK\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "test:coverage": "phpunit --coverage-html coverage/",
    "lint": "parallel-lint --exclude vendor .",
    "phpcs": "phpcs --standard=PSR12 .",
    "phpstan": "phpstan analyse -c phpstan.neon",
    "check": [
      "@lint",
      "@phpcs",
      "@phpstan",
      "@test"
    ]
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
